<%- if controller_name != 'sessions' %>
  <%= link_to "戻る", new_session_path(resource_name),class:"btn btn-danger" %><br />
<% end %>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "パスワードを忘れた方", new_password_path(resource_name),class:"text-danger" %><br />
<% end %>
